<!-- views/profile.ejs -->
<!DOCTYPE html>
<html lang="pt-BR">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Profile</title>
</head>
<body>
  <h1>Welcome, <%= user.firstName %>!</h1>
  <p>Email: <%= user.email %></p>
  <img src="<%= user.picture %>" alt="Profile Picture">
  <a href="/auth/logout">Logout</a>
</body>
</html>